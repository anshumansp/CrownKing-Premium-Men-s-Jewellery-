    "dev:skip-db": "cross-env SKIP_DB=true ts-node src/server.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.{ts,js}",
    "migrate": "ts-node src/migrations/migrate.ts",
    "postinstall": "node scripts/install-types.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
