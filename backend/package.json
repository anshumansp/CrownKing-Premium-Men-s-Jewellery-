    "dev:skip-db": "cross-env SKIP_DB=true ts-node src/server.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.{ts,js}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.9",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.1.0"
